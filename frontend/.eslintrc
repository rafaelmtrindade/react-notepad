{
  "extends": [
    "../.eslintrc",
    "react-app",
    "react-app/jest",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "prettier/prettier": "off",
    "react-hooks/exhaustive-deps": "off"
  },
  "env" : {
    "browser": true
  }
}
